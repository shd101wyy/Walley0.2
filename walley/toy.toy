	;Function: list-append"
	;(list-append '() 12) -> (12)"
	;(list-append '(1 2) '(14 15)) -> (1 2 (14 15))"
	(define list-append (lambda (a b)
			(stms
				(define output (cons b '()))
				(define i (- (list-length a) 1))
				(while (>= i 0)
					(stms
						(set! output (cons (list-get a i) output))
						(set! i (- i 1))
					)
				)
				output
			)
		)
	)
