(begin
	(define toy_eval (lambda (argument)
		(if (null? argument) nil 
			(let 
				(
					; (+ 3 4) -> +
					; get function name
					(function_name (car argument)) 
					; (+ 3 4) -> (3 4)
					; get function parameters
					(function_params (cdr argument))
				)
				(apply function_name function_params)
			)
		)))
	(display (toy_eval '(+ 3 4)))
)